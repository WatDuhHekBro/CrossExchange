{
    "name": "crossexchange",
    "version": "1.2.0",
    "description": "Yet another Discord bot.",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc --project tsconfig.prod.json && npm prune --production",
        "start": "node .",
        "once": "tsc && npm start",
        "dev": "tsc-watch --onSuccess \"node . dev\"",
        "test": "mocha --require ts-node/register --extension ts --recursive",
        "format": "prettier --write **/*",
        "prepare": "husky install"
    },
    "dependencies": {
        "chalk": "^4.1.0",
        "discord.js": "^12.5.3",
        "inquirer": "^8.0.0",
        "onion-lasers": "^1.0.0"
    },
    "devDependencies": {
        "@types/inquirer": "^7.3.1",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.14.37",
        "@types/ws": "^7.4.1",
        "husky": "^6.0.0",
        "mocha": "^8.3.2",
        "prettier": "^2.2.1",
        "ts-node": "^9.1.1",
        "tsc-watch": "^4.2.9",
        "typescript": "^4.2.4"
    },
    "author": "WatDuhHekBro",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/WatDuhHekBro/CrossExchange.git"
    },
    "bugs": {
        "url": "https://github.com/WatDuhHekBro/CrossExchange/issues"
    },
    "homepage": "https://github.com/WatDuhHekBro/CrossExchange#readme"
}
